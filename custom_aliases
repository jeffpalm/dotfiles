alias linkdotfiles="ln -nfs ~/dotfiles/vimrc ~/.vimrc && ln -nfs ~/dotfiles/gvimrc ~/.gvimrc && ln -nfs ~/dotfiles/ideavimrc ~/.ideavimrc && ln -nfs ~/dotfiles/colors ~/.vim/colors"
alias updatealiases="ln -nfs ~/dotfiles/custom_aliases ~/.custom_aliases && source ~/.custom_aliases"
